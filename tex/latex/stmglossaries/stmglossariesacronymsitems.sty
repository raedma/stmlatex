%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Requirements                     %
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% Declare that this style file requires at least LaTeX version 2e.
\NeedsTeXFormat{LaTeX2e}

% Provide the name of your page, the date it was last updated, and a comment about what it's used for
\ProvidesPackage{stmglossariesacronymsitems}[2019/10/27 STMs custom LaTeX acronym definitions]

% If not loaded in advance, load the glossaries package with some default options
\@ifpackageloaded{stmglossariesbase}{}{\RequirePackage{stmglossariesbase}}%

%
\@ifpackageloaded{etoolbox}{}{\RequirePackage{etoolbox}}%

% ---------------------------
% Options
% ---------------------------

% For options
\@ifpackageloaded{kvoptions}{}{\RequirePackage{kvoptions}}%

\SetupKeyvalOptions{%
  family=stmglossariesacronymsitems,%
  prefix=stmglossariesacronymsitems@,%
  setkeys=\kvsetkeys,%
}

% Automatically add the corresponding glossary entry to the acronym
\DeclareBoolOption[false]{autoaddglossaryentry}

% Add a link from the acronym to the glossary entry
\DeclareBoolOption[false]{linktoglossary}

% Process options
\ProcessKeyvalOptions{stmglossariesacronymsitems}

% ---------------------------
% Option handling
% ---------------------------

% If link to glossary we must autoadd
\ifbool{stmglossariesacronymsitems@linktoglossary}{
  \stmglossariesacronymsitems@autoaddglossaryentrytrue
}{}

% If we want to cross-link to the glossary, we need to load the entries
\ifbool{stmglossariesacronymsitems@autoaddglossaryentry}{
  \@ifpackageloaded{stmglossariesglossaryitems}{}{\RequirePackage{stmglossariesglossaryitems}}%
}{}

% ---------------------------
% Commands
% ---------------------------

\newcommand{\seeglossarylinktext}{, \textit{\glossaryname}:}

